<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Registrazione Netflix</title>
</head>
<body>
<div class="reg">
    <div class="intro">
        <p>Accedi con il tuo account Netflix</p>
    </div>
    <div class="reg-form">
        <form action="" method="">
            <label for="email">Email del tuo profilo Netflix:</label>
            <input type="email" id="email" name="email"><br><br>

            <label for="password">Password del tuo profilo Netflix:</label>
            <input type="password" id="password" name="password"><br><br>

            <label for="username">Inserisci il tuo username:</label>
            <input type="text" id="username" name="username" value=""><br><br

            <input type="button" value="Register">
        </form>
    </div>
</div>
</body>
</html>
